{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "vite/client",
      "vue-sweetalert2",
    ],
    "paths": {
      "/@src/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "types/**/*.d.ts",
    "auto-imports.d.ts",
    "typed-router.d.ts",
    "components.d.ts",
    "components-global.d.ts",
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.vue",
    "*.config.ts"
  ],
}